  "devdoc": {
    "details": "This proxy holds the storage of the token contract and delegates every call to the current implementation set.\r Besides, it allows to upgrade the token's behaviour towards further implementations, and provides basic\r authorization control functionalities\r",
    "methods": {
      "implementation()": {
        "details": "Tells the address of the current implementation\r",
        "return": "address of the current implementation\r"
      },
      "proxyOwner()": {
        "details": "Tells the address of the proxy owner\r",
        "return": "the address of the proxy owner\r"
      },
      "transferProxyOwnership(address)": {
        "details": "Allows the current owner to transfer control of the contract to a newOwner.\r",
        "params": {
          "newOwner": "The address to transfer ownership to.\r"
        }
      },
      "upgradeTo(string,address)": {
        "details": "Allows the upgradeability owner to upgrade the current version of the proxy.\r",
        "params": {
          "implementation": "representing the address of the new implementation to be set.\r",
          "version": "representing the version name of the new implementation to be set.\r"
        }
      },
      "upgradeToAndCall(string,address,bytes)": {
        "details": "Allows the upgradeability owner to upgrade the current version of the proxy and call the new implementation\r to initialize whatever is needed through a low level call.\r",
        "params": {
          "data": "represents the msg.data to bet sent in the low level call. This parameter may include the function\r signature of the implementation to be called with the needed payload\r",
          "implementation": "representing the address of the new implementation to be set.\r",
          "version": "representing the version name of the new implementation to be set.\r"
        }
      },
      "upgradeabilityOwner()": {
        "details": "Tells the address of the owner\r",
        "return": "the address of the owner\r"
      },
      "version()": {
        "details": "Tells the version name of the current implementation\r",
        "return": "string representing the name of the current version\r"
      }
    },
    "title": "EternalStorageProxy\r"
  },
